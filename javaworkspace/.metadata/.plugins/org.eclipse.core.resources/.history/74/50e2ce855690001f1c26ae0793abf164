class Car{
		String color;
		int speed; 
		
		void upSpeed(int value) {
			speed = value;
		}
		void downSpeed() {
		
			// 기능을 만든다는건, 메소드를 만든다는 말이다.
		}
}
//두개의 클래스의 범위는 다르다


	
public class Ex01 { // 파일 이름인 것만 Public을 가져야함
	public static void main(String[] args) {
		Car myCar = new Car();
		myCar.color = "파랑";
		myCar.speed = 0;
		myCar.upSpeed(30);
		System.out.println(myCar.speed);
		// 파일 원래 따로 만드는게 정석
		
	}
}
